{{ define "projects/view" }}
{{template "layout/top" }}
<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
    <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
        <h1 class="h2">Releases</h1>
        <div class="btn-toolbar mb-2 mb-md-0">
            <div class="btn-group me-2">
                <a href="/projects/create" class="btn btn-sm btn-outline-info">
                    Create Project
                </a>
            </div>
        </div>
    </div>


    <div class="table-responsive">
        <table class="table table-responsive" id="projects_table"
               data-get_action="/projects/list"
               data-model="releases">
            <thead>
            <tr>
                
                <th>Project Name</th>
                <th>Repo name</th>
                <th>Beta Release</th>
                <th>Regression Signor</th>
                <th>Code Freeze</th>
                <th>Dev Completion</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
            </thead>

        </table>
    </div>
</main>
{{template "layout/bottom" }}
{{end}}